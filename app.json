{
  "expo": {
    "name": "halaqa-passport",
    "slug": "halaqa-passport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Halaqa Passport to scan QR codes for check-in.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.danishmahboob.halaqapassport"
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.danishmahboob.halaqapassport"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e1cfbf46-1097-4e8f-b3df-5f8ec6d08f86"
      }
    }
  }
}
